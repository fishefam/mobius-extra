{
  "manifest_version": 2,
  "name": "mobius-ext",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["*://*.mobius.cloud/qbeditor/*"],
      "js": ["interceptor.js"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": ["interface.js", "interface.css"]
}
